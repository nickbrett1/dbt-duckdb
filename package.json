{
  "name": "dbt-duckdb",
  "version": "1.0.0",
  "scripts": {
    "populate:duckdb": "doppler run -- python populate.py --use-duckdb",
    "populate:postgres": "doppler run -- python populate.py --use-postgres",
    "download:population": "doppler run -- python download_population.py",
    "download:wdi": "doppler run -- python download_wdi.py",
    "dev:export": "doppler run -- python duckdbexport.py --local-d1 --sample-d1",
    "dev:export:full:force": "doppler run -- python duckdbexport.py --local-d1 --force",
    "dev:export:force": "doppler run -- python duckdbexport.py --force --local-d1 --sample-d1",
    "dev:export:ignore-d1": "doppler run -- python duckdbexport.py --ignore-d1 --local-d1"
  },
  "dependencies": {}
}